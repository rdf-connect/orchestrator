@prefix jvm: <https://w3id.org/conn/jvm#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix fnom: <https://w3id.org/function/vocabulary/mapping#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix : <https://w3id.org/conn#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.

<> owl:imports <../pipeline.ttl>.

jvm:Filter a jvm:Processor;
  jvm:file <../sources/Filter.java>;
  jvm:language "Java".

[] a sh:NodeShape;
  sh:targetClass jvm:Filter;
  sh:property [
    sh:path jvm:whitelist;
    sh:name "whitelist";
    sh:datatype xsd:integer;
  ], [
    sh:path jvm:input;
    sh:name "input";
    sh:class jvm:ChannelReader;
    sh:minCount 1;
    sh:maxCount 1;
  ], [
    sh:path jvm:output;
    sh:name "output";
    sh:class jvm:ChannelWriter;
    sh:minCount 1;
    sh:maxCount 1;
  ];
  sh:closed true;
  sh:ignoredProperties (rdf:type).
