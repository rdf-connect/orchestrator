@prefix jvm: <https://w3id.org/conn/jvm#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

jvm:NodeJSLogger a jvm:NodeJSProcessor ;
    jvm:class "Logger" ;
    jvm:package "" ;
    jvm:src <file:///Users/jens/Developer/technology.idlab.jvm-runner/examples/processors/logger-ts/build/index.js> .

[] a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class jvm:Reader ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "input" ;
            sh:path jvm:input ],
        [ sh:class jvm:Writer ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "output" ;
            sh:path jvm:output ] ;
    sh:targetClass jvm:NodeJSLogger .

